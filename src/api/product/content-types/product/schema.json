{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "condition": {
      "type": "integer",
      "required": true,
      "max": 10,
      "min": 1
    },
    "minusInfo": {
      "type": "string",
      "required": false
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "size": {
      "type": "string",
      "required": true
    },
    "images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    }
  }
}
